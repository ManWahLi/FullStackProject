<div class="container mt-3" id="<%= dom_id brand %>">
  <h1>
    <strong>
      <%= brand.brand_name %>
    </strong>
  </h1>
  <h3>Products by Brand (Order by Rating)</h3>
  <table class="table">
    <%= render partial: 'products/thead' %>
    <tbody>
      <%= render partial: 'products/tbody', collection: brand.products.order("rating DESC"), as: :product %>
    </tbody>
  </table>
</div>

