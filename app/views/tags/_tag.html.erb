<div class="m-3" id="<%= dom_id tag %>">
  <h1>
    <strong>
      <%= tag.tag_name %>
    </strong>
  </h1>
  <h3>Products by Tag (Order by Rating)</h3>
  <table class="table">
    <%= render partial: 'products/thead' %>
    <tbody>
      <%= render partial: 'products/tbody', collection: tag.products.order("rating DESC"), as: :product %>
    </tbody>
  </table>
</div>


